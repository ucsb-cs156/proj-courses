{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "=v20.16.0"
  },
  "dependencies": {
    "@microlink/react-json-view": "^1.23.1",
    "axios": "^1.7.3",
    "bootstrap": "^5.3.3",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.1",
    "react-table": "^7.8.0",
    "react-toastify": "^10.0.5",
    "recharts": "^2.12.7",
    "save-dev": "^0.0.1-security"
  },
  "scripts": {
    "start": "env-cmd -f ../.env  -e development react-scripts start",
    "build": "env-cmd -f ../.env --silent react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "storybook": "start-storybook --docs -p 6006 -s public",
    "build-storybook": "build-storybook --docs -s public",
    "check-format": "prettier --check \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!(axios)/)"
    ],
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/main/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "axios-mock-adapter": "^2.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest-mock-console": "^2.0.0",
    "prettier": "^3.3.3",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.3.1"
  }
}
