{
  "name": "proj-courses-frontend",
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "=v20.17.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.4",
    "bootstrap": "^5.3.3",
    "graphql": "^16.9.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.3.1",
    "react-fontawesome": "^1.7.1",
    "react-hook-form": "^7.53.0",
    "react-inspector": "^6.0.2",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.1",
    "react-toastify": "^10.0.5",
    "react18-json-view": "^0.2.8",
    "recharts": "^2.12.7",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "test:watch:coverage": "vitest --coverage",
    "check-format": "prettier --check \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic run --project-token=$(grep CHROMATIC_PROJECT_TOKEN= ../.env  | sed -e s/CHROMATIC_PROJECT_TOKEN=// ) --exit-once-uploaded"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@chromatic-com/storybook": "^1.7.0",
    "@eslint/js": "^9.9.0",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/addon-queryparams": "^7.0.1",
    "@storybook/blocks": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/test": "^8.2.9",
    "@stryker-mutator/vitest-runner": "^8.5.0",
    "@testing-library/react": "^16.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "axios-mock-adapter": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.9.0",
    "jest-mock-console": "^2.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.4.1",
    "msw-storybook-addon": "^2.0.3",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "storybook": "^8.2.9",
    "storybook-addon-remix-react-router": "^3.0.0",
    "typescript-eslint": "^8.2.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "branches": 80,
    "functions": 80,
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "jest": {
    "moduleNameMapping": {
      "^axios$": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "src/main/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_|",
          "varsIgnorePattern": "(^_)|(^React$)"
        }
      ],
      "react/react-in-jsx-scope": "off",
      "react/jsx-uses-react": "off",
      "react/prop-types": "off",
      "react/no-unescaped-entities": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "(^_)|(^React$)"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "overrides": {
    "react-refresh": "0.14.2"
  }
}
